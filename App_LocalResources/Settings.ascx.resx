  <data name="Settings.Template" xml:space="preserve">
    <value>&lt;div class="reg_Container"&gt;

    &lt;div class="reg_Field"&gt;
       [LABEL:AlreadySubscribed]
    &lt;/div&gt;

    &lt;div class="reg_Field"&gt;
      &lt;div class="reg_Label"&gt;[MERGE:EMAIL:LABEL]&lt;/div&gt;
      &lt;div class="reg_Control"&gt;[MERGE:EMAIL:CONTROL]&lt;/div&gt;
      &lt;div class="reg_Required"&gt;[MERGE:EMAIL:REQUIRED]&lt;/div&gt;
      &lt;div class="clearfix"&gt;&lt;/div&gt;
    &lt;/div&gt;

    &lt;div class="reg_Field"&gt;
      &lt;div class="reg_Label"&gt;[MERGE:FNAME:LABEL]&lt;/div&gt;
      &lt;div class="reg_Control"&gt;[MERGE:FNAME:CONTROL]&lt;/div&gt;
      &lt;div class="reg_Required"&gt;[MERGE:FNAME:REQUIRED]&lt;/div&gt;
      &lt;div class="clearfix"&gt;&lt;/div&gt;
    &lt;/div&gt;

    &lt;div class="reg_Field"&gt;
      &lt;div class="reg_Label"&gt;[MERGE:LNAME:LABEL]&lt;/div&gt;
      &lt;div class="reg_Control"&gt;[MERGE:LNAME:CONTROL]&lt;/div&gt;
      &lt;div class="reg_Required"&gt;[MERGE:LNAME:REQUIRED]&lt;/div&gt;
      &lt;div class="clearfix"&gt;&lt;/div&gt;
    &lt;/div&gt;

    &lt;div class="reg_Field"&gt;
      &lt;div class="reg_Label"&gt;[MERGE:OPTIONS:LABEL]&lt;/div&gt;
      &lt;div class="reg_Control"&gt;[MERGE:OPTIONS:CONTROL]&lt;/div&gt;  
      &lt;div class="clearfix"&gt;&lt;/div&gt;
    &lt;/div&gt;

    &lt;div class="reg_Command"&gt;
      &lt;div class="Left"&gt;[BUTTON:UpdateSettings]&amp;nbsp;[BUTTON:UnSubscribe]&lt;/div&gt;      
      &lt;div class="ucmClear"&gt;&lt;/div&gt;
    &lt;/div&gt;

&lt;/div&gt;</value>
  </data>
